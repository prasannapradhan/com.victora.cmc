{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.victora.cmc.uix",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.8"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.0",
      "toolsId": "5e738eae-30af-4f42-97d9-3fece69cf237"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.128.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.victora.cmc.uix.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.victora.cmc.uix.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearTarget" : "true"
      },
      "routes": [

        {
          "pattern": "",
          "name": "InitialLoader",
          "target": "InitialLoader"
      },
        {
          "name": "CustomerListing",
          "pattern": "CustomerListing",
          "target": [
            "TargetcustomerListing"
          ]
        },
        {
          "name": "VendorListing",
          "pattern": "VendorListing",
          "target": [
            "TargetVendorListing"
          ]
        },
        {
          "name": "MaterialListing",
          "pattern": "MaterialListing",
          "target": [
            "MaterialListing"
          ]
        }
      ],
    "targets": {
  "InitialLoader": {
    "viewType": "XML",
    "transition": "slide",
    "clearControlAggregation": true,
    "viewId": "InitialLoader",
    "viewName": "InitialLoader"
  },

  "TargetcustomerListing": {
    "viewType": "XML",
    "transition": "slide",
    "clearControlAggregation": false,
    "viewId": "CustomerListing",
    "viewName": "CustomerListing"
  },
  "TargetVendorListing": {
    "viewType": "XML",
    "transition": "slide",
    "clearControlAggregation": true,
    "viewId": "VendorListing",
    "viewName": "VendorListing"
  },
  "MaterialListing": {
    "viewType": "XML",
    "transition": "slide",
    "clearControlAggregation": true,
    "viewId": "MaterialListing",
    "viewName": "MaterialListing"
  }
}
    },
    "rootView": {
      "viewName": "com.victora.cmc.uix.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "mrouter"
  }
}
